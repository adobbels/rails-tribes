<h1>Reviews#new</h1>
<p>Find me in app/views/reviews/new.html.erb</p>
<%= simple_form_for [@house, @review] do |f| %>
        <%= f.input :title_review %>
        <%= f.input :content_review %>
        <%= f.input :rating, collection: 0..5 %>
        <%= f.submit "add a review", class: "btn btn-primary" %>
<% end %>
