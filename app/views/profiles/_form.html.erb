<%= simple_form_for(@profile) do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :address %>
  <%= f.input :post_code %>
  <%= f.input :city %>
  <%= f.input :phone_number %>
  <%= f.input :birthday %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
<% end %>

